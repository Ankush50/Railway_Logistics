{
  "name": "railway-logistics-backend",
  "version": "1.0.0",
  "description": "Railway Logistics API server with enhanced security",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "security:audit": "npm audit && npm audit fix",
    "security:check": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix --force",
    "test:security": "npm run security:check"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "keywords": ["railway", "logistics", "api", "express", "mongodb", "security"],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
